# Example .env file for web dashboard
# Copy to .env and customize

# Backend API Configuration
DASH_BACKEND_URL=http://localhost:8000
DASH_API_VERSION=v1

# Server Configuration
DASH_DEBUG=true
DASH_HOST=0.0.0.0
DASH_PORT=8050

# API Timeouts (seconds)
DASH_HEALTH_CHECK_TIMEOUT=5.0
DASH_ANALYZE_TIMEOUT=30.0

# Image Processing
DASH_MAX_IMAGE_SIZE_MB=10
DASH_SUPPORTED_FORMATS=jpg,jpeg,png,webp

# Application Settings
DASH_ENABLE_CORS=true
DASH_LOG_LEVEL=INFO
